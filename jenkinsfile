@Library("shared") _
pipeline {
    agent any

    stages {
        stage('not hello'){
            steps {
                script {
                    hello()
                }
            }
        }
        stage('Hello') {
            steps {
                echo 'Hello mutthuu-don'
                sh ''' 
                mkdir bird
                cd meow 
                echo "Hello World" > test.txt
                '''
            }
        }
    }
}
